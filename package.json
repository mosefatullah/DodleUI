{
  "name": "@mosefatullah/doodleui",
  "version": "0.0.3",
  "description": "A CSS & JavaScript framework to execute functionalities for UI improvement.",
  "homepage": "https://github.com/mosefatullah/doodleui#readme",
  "private": false,
  "main": "dist/doodleui.umd.js",
  "module": "dist/doodleui.es.js",
  "exports": {
    ".": {
      "import": "./dist/doodleui.es.js",
      "require": "./dist/doodleui.umd.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mosefatullah/doodleui.git"
  },
  "scripts": {
    "dev": "vite",
    "sass:start": "sass --watch scss/doodleui.scss:dist/doodleui.css",
    "sass:build": "sass --no-source-map scss/doodleui.scss:dist/doodleui.css && sass --no-source-map scss/doodleui.scss:dist/doodleui.min.css --style=compressed",
    "build": "vite build && npm run sass:build",
    "preview": "vite preview"
  },
  "keywords": [
    "doodleui",
    "ui",
    "npm",
    "package",
    "css",
    "js"
  ],
  "author": "Mohammad Sefatullah",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.69.5"
  },
  "bugs": {
    "url": "https://github.com/mosefatullah/doodleui/issues"
  }
}